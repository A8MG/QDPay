<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<mapper namespace="com.a8.qdm.query.dao.GameDeviceDao">
	<parameterMap type="com.a8.qdm.query.dao.bean.GameDevice"
		id="gameDevice" />

	<insert id="addGameDevice" parameterMap="gameDevice">
		insert into game_device
		(gameid, deviceid) values (#{gameId}, #{deviceId})
	</insert>
</mapper>